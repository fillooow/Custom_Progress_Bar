<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="240dp"
    android:layout_height="240dp"
    tools:layout_gravity="center"
    tools:parentTag="android.widget.FrameLayout">

    <fillooow.app.customprogressbar.custom_view.radial.RadialSpeedometerProgressView
        android:id="@+id/tochka_radial_speedometer_progress_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center" />

    <fillooow.app.customprogressbar.custom_view.text.TochkaTextView
        android:id="@+id/tochka_radial_speedometer_view_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textAlignment="center"
        android:layout_marginTop="@dimen/space_15"
        android:layout_marginBottom="@dimen/space_15"
        android:layout_marginStart="@dimen/space_8"
        android:layout_marginEnd="@dimen/space_8"
        android:textColor="@color/kit_grey_400"
        app:viewStyle="TS500_XL"
        app:removeDanglingPrepositions="false"
        tools:text="Нет операций" />

</merge>